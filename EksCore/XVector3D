#ifndef XVECTOR3D_H
#define XVECTOR3D_H

#include "XGlobal"
#include "QVector3D"
#include "XMetaType"
#include "XList"

typedef QVector3D XVector3D;
typedef XList<XVector3D> XVector3DList;

class QTextStream;
EKSCORE_EXPORT QTextStream &operator>>( QTextStream &str, XVector3D &vec );
EKSCORE_EXPORT QTextStream &operator<<( QTextStream &str, const XVector3D &vec );

X_DECLARE_SERIALISABLE_METATYPE(XVector3D);

#endif // XVECTOR3D_H
